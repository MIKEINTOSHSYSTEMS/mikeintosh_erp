<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    action for export pdf without group by-->
    <record id="ir_exports_pdf_report" model="ir.actions.report">
        <field name="name">Export</field>
        <field name="model">ir.exports</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">export_view_pdf.export_in_pdf</field>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="paperformat_export"/>
    </record>

    <!--    action for export pdf with group by-->
    <record id="ir_exports_pdf_group_by_report" model="ir.actions.report">
        <field name="name">Export</field>
        <field name="model">ir.exports</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">export_view_pdf.export_in_pdf_group_by</field>
    </record>
</odoo>
